int parse_str(char str[]){
	char token;
	int sum=0,length,i;
	length=strlen(str);
	for(i=0;i<length-1;i++){
		if(str[i]<'0'||str[i]>'9') return -1;
		else sum+=i*(str[i]-0);
	}
	if((sum%10)==(str[length-1]-'0')) return 1;
	else return 0;
}
